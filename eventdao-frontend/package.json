{
  "name": "eventdao",
  "version": "1.0.0",
  "description": "Platform event terdesentralisasi di Solana blockchain",
  "private": true,
  "scripts": {
    "dev": "cd eventdao-frontend && npm run dev",
    "build": "cd eventdao-frontend && npm run build",
    "start": "cd eventdao-frontend && npm run start",
    "test": "cd eventdao-frontend && npm run test",
    "anchor:build": "cd eventdao-anchor/eventdao && anchor build",
    "anchor:test": "cd eventdao-anchor/eventdao && anchor test",
    "anchor:deploy": "cd eventdao-anchor/eventdao && anchor deploy",
    "setup": "cd eventdao-frontend && npm install && cd ../eventdao-anchor/eventdao && npm install",
    "clean": "rm -rf eventdao-frontend/node_modules eventdao-anchor/eventdao/node_modules"
  },
  "keywords": [
    "solana",
    "web3",
    "blockchain",
    "events",
    "dao",
    "rust",
    "nextjs",
    "typescript"
  ],
  "author": "EventDAO Team",
  "license": "MIT"
}
